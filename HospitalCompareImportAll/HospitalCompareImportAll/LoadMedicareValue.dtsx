                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT [Provider Number] AS ProviderID
	  , 2009 AS DataYear
	  ,[MS-DRG ID] AS DRGId
	  ,[Medicare Median Payment] AS AveragePayment
	  ,CASE WHEN [Number Of Cases] = '*' THEN null else [Number Of Cases] end AS Denominator
	  ,null AS MeasureStartDate
	  ,null AS MeasureEndDate
FROM [HospitalCompareStage].[dbo].[2009_HQI_HOSP_MPV_MSR]
UNION
SELECT SUBSTRING(RTRIM(LTRIM([Provider Number])), 2, 6)
	  , 2010 AS DataYear
	  ,[Diagnosis Related Group ID]
	  ,[Medicare Average Payment] AS AveragePayment
	  ,CASE WHEN [Number Of Cases] = '*' THEN null else [Number Of Cases] end AS Denominator
	  ,null AS MeasureStartDate
	  ,null AS MeasureEndDate
FROM [HospitalCompareStage].[dbo].[2010_HQI_HOSP_MPV_MSR]
UNION
SELECT SUBSTRING(RTRIM(LTRIM([Provider Number])), 2, 6)
	  , 2011 AS DataYear
	  ,[Diagnosis Related Group ID]
	  ,[Medicare Average Payment] AS AveragePayment
	  ,CASE WHEN [Number Of Cases] = '*' THEN null else [Number Of Cases] end AS Denominator
	  ,null AS MeasureStartDate
