      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{519027B6-004D-431F-A052-1086E046AF88}"
          SQLTask:SqlStatementSource="USE [UNRHealthCareData]&#xA;&#xA;DECLARE&#x9;&#xA;@cols AS NVARCHAR(MAX)&#xA;, @cols1 AS NVARCHAR(MAX)&#xA;, @cols2 AS NVARCHAR(MAX)&#xA;, @query1 AS NVARCHAR(MAX)&#xA;, @query2 AS NVARCHAR(MAX)&#xA;, @queryFinal AS NVARCHAR(MAX)&#xA;&#xA;IF OBJECT_ID('vMedicareValuePivot', 'V') IS NOT NULL&#xA;DROP VIEW vMedicareValuePivot;&#xA;&#xA;select @cols = STUFF((SELECT ',' + QUOTENAME(mis.[DRGCodeId]) &#xA;                    FROM [UNRHealthcareData].[dbo].[MedicareValue] mis&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[DRGCode] m on m.DRGCodeId = mis.DRGCodeId&#xA;                    group by mis.[DRGCodeId]&#xA;            FOR XML PATH(''), TYPE&#xA;            ).value('.', 'NVARCHAR(MAX)') &#xA;        ,1,1,'')&#xA;&#xA;SET @cols1 = REPLACE(@cols, '[', '[DRGId_')&#xA;SET @cols1 = REPLACE(@cols1, ']', '_AveragePayment]')&#xA;&#xA;--select @cols1&#xA;&#xA;&#xA;set @query1 = 'SELECT p.ProviderId, p.DataYear, ' + @cols1 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, ''DRGId_'' + CAST(mis.DRGCodeId AS nvarchar) + ''_AveragePayment'' AS DRGCodeIdRename, AveragePayment&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[MedicareValue] mis&#x9;&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[DRGCode] m on m.DRGCodeId = mis.DRGCodeId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(AveragePayment)&#xA;&#x9;FOR DRGCodeIdRename IN (' + @cols1 + ')&#xA;)p'&#xA;&#xA;SET @cols2 = REPLACE(@cols, '[', '[DRGId_')&#xA;SET @cols2 = REPLACE(@cols2, ']', '_Denominator]')&#xA;&#xA;--select @cols2&#xA;&#xA;&#xA;set @query2 = 'SELECT p.ProviderId, p.DataYear, ' + @cols2 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, ''DRGId_'' + CAST(mis.DRGCodeId AS nvarchar) + ''_Denominator'' AS DRGCodeIdRename, Denominator&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[MedicareValue] mis&#x9;&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[DRGCode] m on m.DRGCodeId = mis.DRGCodeId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(Denominator)&#xA;&#x9;FOR DRGCodeIdRename IN (' + @cols2 + ')&#xA;)p'&#xA;&#xA;--select @query2&#xA;&#xA;SET @queryFinal = 'CREATE VIEW vMedicareValuePivot AS&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT a.ProviderId, a.DataYear, ' + @cols1 + ', ' + @cols2 + ' &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;FROM (' + @query1 + ') a &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;LEFT JOIN (' + @query2 + ') b ON b.ProviderId = a.ProviderId AND b.DataYear = a.DataYear'&#xA;&#xA;--select @queryfinal&#xA;execute(@queryFinal)&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
