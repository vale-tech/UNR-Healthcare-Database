        <SQLTask:SqlTaskData
          SQLTask:Connection="{519027B6-004D-431F-A052-1086E046AF88}"
          SQLTask:SqlStatementSource="USE [UNRHealthCareData]&#xA;&#xA;DECLARE&#x9;&#xA;@cols AS NVARCHAR(MAX)&#xA;, @cols1 AS NVARCHAR(MAX)&#xA;, @cols2 AS NVARCHAR(MAX)&#xA;, @cols3 AS NVARCHAR(MAX)&#xA;, @cols4 AS NVARCHAR(MAX)&#xA;, @cols5 AS NVARCHAR(MAX)&#xA;, @query1 AS NVARCHAR(MAX)&#xA;, @query2 AS NVARCHAR(MAX)&#xA;, @query3 AS NVARCHAR(MAX)&#xA;, @query4 AS NVARCHAR(MAX)&#xA;, @query5 AS NVARCHAR(MAX)&#xA;, @queryFinal AS NVARCHAR(MAX)&#xA;&#xA;IF OBJECT_ID('vHVBPHCAHPSScorePivot', 'V') IS NOT NULL&#xA;DROP VIEW vHVBPHCAHPSScorePivot;&#xA;&#xA;select @cols = STUFF((SELECT ',' + QUOTENAME('MId_' + CAST(mis.[HVBPHCAHPSMeasureId] AS nvarchar)) &#xA;                    FROM [UNRHealthcareData].[dbo].[HVBPHCAHPSScore] mis&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[HVBPHCAHPSMeasure] m on m.HVBPHCAHPSMeasureId = mis.HVBPHCAHPSMeasureId&#xA;                    group by mis.[HVBPHCAHPSMeasureId]&#xA;            FOR XML PATH(''), TYPE&#xA;            ).value('.', 'NVARCHAR(MAX)') &#xA;        ,1,1,'')&#xA;&#xA;SET @cols1 = REPLACE(@cols, ']', '_AchievementPoints]')&#xA;&#xA;set @query1 = 'SELECT p.ProviderId, p.DataYear, ' + @cols1 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, ''MId_'' + CAST(mis.[HVBPHCAHPSMeasureId] AS nvarchar) + ''_AchievementPoints'' AS MeasureNameRename, AchievementPoints&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[HVBPHCAHPSScore] mis&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[HVBPHCAHPSMeasure] m on m.HVBPHCAHPSMeasureId = mis.HVBPHCAHPSMeasureId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(AchievementPoints)&#xA;&#x9;FOR MeasureNameRename IN (' + @cols1 + ')&#xA;)p'&#xA;&#xA;--select @query1&#xA;&#xA;SET @cols2 = REPLACE(@cols, ']', '_ImprovementPoints]')&#xA;&#xA;set @query2 = 'SELECT p.ProviderId, p.DataYear, ' + @cols2 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, ''MId_'' + CAST(mis.[HVBPHCAHPSMeasureId] AS nvarchar) + ''_ImprovementPoints'' AS MeasureNameRename, ImprovementPoints&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[HVBPHCAHPSScore] mis&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[HVBPHCAHPSMeasure] m on m.HVBPHCAHPSMeasureId = mis.HVBPHCAHPSMeasureId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(ImprovementPoints)&#xA;&#x9;FOR MeasureNameRename IN (' + @cols2 + ')&#xA;)p'&#xA;&#xA;SET @cols3 = REPLACE(@cols, ']', '_DimensionScore]')&#xA;&#xA;set @query3 = 'SELECT p.ProviderId, p.DataYear, ' + @cols3 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, ''MId_'' + CAST(mis.[HVBPHCAHPSMeasureId] AS nvarchar) + ''_DimensionScore'' AS MeasureNameRename, DimensionScore&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[HVBPHCAHPSScore] mis&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[HVBPHCAHPSMeasure] m on m.HVBPHCAHPSMeasureId = mis.HVBPHCAHPSMeasureId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(DimensionScore)&#xA;&#x9;FOR MeasureNameRename IN (' + @cols3 + ')&#xA;)p'&#xA;&#xA;&#xA;SET @queryFinal = 'CREATE VIEW vHVBPHCAHPSScorePivot AS&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT a.ProviderId, a.DataYear, ' + @cols1 + ', ' + @cols2 + ', ' + @cols3 + '&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;FROM (' + @query1 + ') a &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN (' + @query2 + ') b ON b.ProviderId = a.ProviderId AND b.DataYear = a.DataYear&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN (' + @query3 + ') c ON c.ProviderId = a.ProviderId AND c.DataYear = a.DataYear'&#xA;&#xA;&#xA;&#xA;--select @queryfinal&#xA;execute(@queryFinal)&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
