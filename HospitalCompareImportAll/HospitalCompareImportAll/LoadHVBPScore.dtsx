                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @HVPBData AS TABLE
(
	ProviderId varchar(10)
	, DataYear int
	, MeasureCode varchar(100)
	, PerformanceRate varchar(100)
	, AchievementPoints varchar(100)
	, ImprovementPoints varchar(100)
	, MeasureScore varchar(100)
)

--HAI
INSERT INTO @HVPBData
  SELECT [Provider Number], 2013 AS DataYear, UPPER(REPLACE(LEFT(MeasureCode1, CHARINDEX(' ', MeasureCode1) - 1), '-', '_')) As MeasureCode, PerformanceRate, AchievementPoints, ImprovementPoints, MeasureScore
  FROM 
	(SELECT [Provider Number], [SCIP-Inf-1 Performance Rate]
      ,[SCIP-Inf-2 Performance Rate]
      ,[SCIP-Inf-3 Performance Rate]
      ,[SCIP-Inf-4 Performance Rate]
	  ,[SCIP-Inf-1 Achievement Points]
      ,[SCIP-Inf-2 Achievement Points]
      ,[SCIP-Inf-3 Achievement Points]
      ,[SCIP-Inf-4 Achievement Points]
	  ,[SCIP-Inf-1 Improvement Points]
      ,[SCIP-Inf-2 Improvement Points]
      ,[SCIP-Inf-3 Improvement Points]
      ,[SCIP-Inf-4 Improvement Points]
	  ,[SCIP-Inf-1 Measure Score]
      ,[SCIP-Inf-2 Measure Score]
      ,[SCIP-Inf-3 Measure Score]
      ,[SCIP-Inf-4 Measure Score]
      FROM [HospitalCompareStage].[dbo].[2013_Hvbp_hai_05_28_2013]) p
 UNPIVOT
  (PerformanceRate FOR MeasureCode1 IN ([SCIP-Inf-1 Performance Rate]
      ,[SCIP-Inf-2 Performance Rate]
      ,[SCIP-Inf-3 Performance Rate]
      ,[SCIP-Inf-4 Performance Rate])) AS a 
UNPIVOT
  (AchievementPoints FOR MeasureCode2 IN ([SCIP-Inf-1 Achievement Points]
      ,[SCIP-Inf-2 Achievement Points]
      ,[SCIP-Inf-3 Achievement Points]
      ,[SCIP-Inf-4 Achievement Points])) AS b
UNPIVOT
  (ImprovementPoints FOR MeasureCode3 IN ([SCIP-Inf-1 Improvement Points]
      ,[SCIP-Inf-2 Improvement Points]
      ,[SCIP-Inf-3 Improvement Points]
      ,[SCIP-Inf-4 Improvement Points])) AS c
UNPIVOT
  (MeasureScore FOR MeasureCode4 IN ([SCIP-Inf-1 Measure Score]
      ,[SCIP-Inf-2 Measure Score]
      ,[SCIP-Inf-3 Measure Score]
      ,[SCIP-Inf-4 Measure Score])) AS d
WHERE 
	LEFT(MeasureCode1, CHARINDEX(' ', MeasureCode1)) = LEFT(MeasureCode2, CHARINDEX(' ', MeasureCode2))  
AND
	LEFT(MeasureCode1, CHARINDEX(' ', MeasureCode1)) = LEFT(MeasureCode3, CHARINDEX(' ', MeasureCode3)) 
AND
	LEFT(MeasureCode1, CHARINDEX(' ', MeasureCode1)) = LEFT(MeasureCode4, CHARINDEX(' ', MeasureCode4)) 
UNION
SELECT [Provider Number], 2014 AS DataYear, UPPER(REPLACE(LEFT(MeasureCode1, CHARINDEX('_', MeasureCode1) - 1), '-', '_')) As MeasureCode, PerformanceRate, AchievementPoints, ImprovementPoints, MeasureScore
  FROM 
	(SELECT [Provider Number], [SCIP-Inf-1_Performance Rate]
      ,[SCIP-Inf-2_Performance Rate]
      ,[SCIP-Inf-3_Performance Rate]
      ,[SCIP-Inf-4_Performance Rate]
	  ,[SCIP-Inf-9_Performance Rate]
	  ,[SCIP-Inf-1_Achievement Points]
      ,[SCIP-Inf-2_Achievement Points]
      ,[SCIP-Inf-3_Achievement Points]
      ,[SCIP-Inf-4_Achievement Points]
	  ,[SCIP-Inf-9_Achievement Points]
	  ,[SCIP-Inf-1_Improvement Points]
      ,[SCIP-Inf-2_Improvement Points]
      ,[SCIP-Inf-3_Improvement Points]
      ,[SCIP-Inf-4_Improvement Points]
	  ,[SCIP-Inf-9_Improvement Points]
	  ,[SCIP-Inf-1_Measure Score]
      ,[SCIP-Inf-2_Measure Score]
      ,[SCIP-Inf-3_Measure Score]
      ,[SCIP-Inf-4_Measure Score]
	  ,[Scip-Inf-9_Measure Score]
      FROM [HospitalCompareStage].[dbo].[2014_Hvbp_hai_10_28_2014]) p
 UNPIVOT
  (PerformanceRate FOR MeasureCode1 IN ([SCIP-Inf-1_Performance Rate]
      ,[SCIP-Inf-2_Performance Rate]
      ,[SCIP-Inf-3_Performance Rate]
      ,[SCIP-Inf-4_Performance Rate]
	  ,[SCIP-Inf-9_Performance Rate])) AS a 
UNPIVOT
  (AchievementPoints FOR MeasureCode2 IN ([SCIP-Inf-1_Achievement Points]
      ,[SCIP-Inf-2_Achievement Points]
      ,[SCIP-Inf-3_Achievement Points]
      ,[SCIP-Inf-4_Achievement Points]
	  ,[SCIP-Inf-9_Achievement Points])) AS b
UNPIVOT
  (ImprovementPoints FOR MeasureCode3 IN ([SCIP-Inf-1_Improvement Points]
      ,[SCIP-Inf-2_Improvement Points]
      ,[SCIP-Inf-3_Improvement Points]
      ,[SCIP-Inf-4_Improvement Points]
	  ,[SCIP-Inf-9_Improvement Points])) AS c
UNPIVOT
  (MeasureScore FOR MeasureCode4 IN ([SCIP-Inf-1_Measure Score]
      ,[SCIP-Inf-2_Measure Score]
      ,[SCIP-Inf-3_Measure Score]
      ,[SCIP-Inf-4_Measure Score]
	  ,[SCIP-Inf-9_Measure Score])) AS d
WHERE 
	LEFT(MeasureCode1, CHARINDEX('_', MeasureCode1)) = LEFT(MeasureCode2, CHARINDEX('_', MeasureCode2))  
AND
