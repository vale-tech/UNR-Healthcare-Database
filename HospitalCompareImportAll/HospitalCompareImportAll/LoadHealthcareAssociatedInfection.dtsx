INSERT INTO @Full (ProviderID, DataYear, MeasureCode, StandardizedInfectionRatio
						, ConfidenceIntervalLower, ConfidenceIntervalHigher
						, Numerator, EligibleCases
						, NumberofDaysorPatients
						, NationalComparisonCategory, MeasureStartDate, MeasureEndDate
						)
SELECT 
b.[prvdr_id]
, 2012
,RTRIM(UPPER(REPLACE(b.[msr_cd], '_SIR', '')))
,b.[scr]
,s.scr
, s1.scr
, s2.scr
, s3.scr
, s4.scr
, 'Missing', null, null
FROM [HospitalCompareStage].[dbo].[2012_HQI_HOSP_HAI] b
INNER JOIN (SELECT [prvdr_id], [scr], [msr_cd] FROM [2012_HQI_HOSP_HAI] WHERE [msr_cd] LIKE '%_LOWER') s on s.[prvdr_id] = b.[prvdr_id] AND RTRIM(UPPER(REPLACE(b.[msr_cd], '_SIR', ''))) + '_CI_LOWER' = s.[msr_cd]
INNER JOIN (SELECT [prvdr_id], [scr], [msr_cd] FROM [2012_HQI_HOSP_HAI] WHERE [msr_cd] LIKE '%_UPPER') s1 on s1.[prvdr_id] = b.[prvdr_id] AND RTRIM(UPPER(REPLACE(b.[msr_cd], '_SIR', ''))) + '_CI_UPPER' = s1.[msr_cd]
INNER JOIN (SELECT [prvdr_id], [scr], [msr_cd] FROM [2012_HQI_HOSP_HAI] WHERE [msr_cd] LIKE '%_NUMERATOR') s2 on s2.[prvdr_id] = b.[prvdr_id] AND RTRIM(UPPER(REPLACE(b.[msr_cd], '_SIR', ''))) + '_NUMERATOR' = s2.[msr_cd]
INNER JOIN (SELECT [prvdr_id], [scr], [msr_cd] FROM [2012_HQI_HOSP_HAI] WHERE [msr_cd] LIKE '%_ELIGCASES') s3 on s3.[prvdr_id] = b.[prvdr_id] AND RTRIM(UPPER(REPLACE(b.[msr_cd], '_SIR', ''))) + '_ELIGCASES' = s3.[msr_cd]
INNER JOIN (SELECT [prvdr_id], [scr], [msr_cd] FROM [2012_HQI_HOSP_HAI] WHERE ([msr_cd] LIKE '%_DEVICEDAYS' OR [msr_cd] LIKE '%PROCDAYS')) s4 on s4.[prvdr_id] = b.[prvdr_id] AND (RTRIM(UPPER(REPLACE(b.[msr_cd], '_SIR', ''))) + '_DEVICEDAYS' = s4.[msr_cd] OR RTRIM(UPPER(REPLACE(b.[msr_cd], '_SIR', ''))) + '_PROCDAYS' = s4.[msr_cd])
WHERE 

