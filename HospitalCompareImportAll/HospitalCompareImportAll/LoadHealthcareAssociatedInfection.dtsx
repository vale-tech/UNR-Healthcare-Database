      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{519027B6-004D-431F-A052-1086E046AF88}"
          SQLTask:SqlStatementSource="USE [UNRHealthCareData]&#xA;&#xA;DECLARE&#x9;&#xA;@cols AS NVARCHAR(MAX)&#xA;, @cols1 AS NVARCHAR(MAX)&#xA;, @cols2 AS NVARCHAR(MAX)&#xA;, @cols3 AS NVARCHAR(MAX)&#xA;, @cols4 AS NVARCHAR(MAX)&#xA;, @cols5 AS NVARCHAR(MAX)&#xA;, @cols6 AS NVARCHAR(MAX)&#xA;, @query1 AS NVARCHAR(MAX)&#xA;, @query2 AS NVARCHAR(MAX)&#xA;, @query3 AS NVARCHAR(MAX)&#xA;, @query4 AS NVARCHAR(MAX)&#xA;, @query5 AS NVARCHAR(MAX)&#xA;, @query6 AS NVARCHAR(MAX)&#xA;, @queryFinal AS NVARCHAR(MAX)&#xA;&#xA;IF OBJECT_ID('vHealthcareAssociatedInfectionScorePivot', 'V') IS NOT NULL&#xA;DROP VIEW vHealthcareAssociatedInfectionScorePivot;&#xA;&#xA;select @cols = STUFF((SELECT ',' + QUOTENAME([MeasureCode]) &#xA;                    FROM [UNRHealthcareData].[dbo].[HealthcareAssociatedInfectionScore] mis&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[Measure] m on m.MeasureId = mis.MeasureId&#xA;                    group by [MeasureCode]&#xA;            FOR XML PATH(''), TYPE&#xA;            ).value('.', 'NVARCHAR(MAX)') &#xA;        ,1,1,'')&#xA;&#xA;SET @cols1 = REPLACE(@cols, ']', '_StandardizedInfectionRatio]')&#xA;&#xA;set @query1 = 'SELECT p.ProviderId, p.DataYear, ' + @cols1 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, MeasureCode + ''_StandardizedInfectionRatio'' AS MeasureCodeRename, StandardizedInfectionRatio&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[HealthcareAssociatedInfectionScore] mis&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[Measure] m on m.MeasureId = mis.MeasureId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(StandardizedInfectionRatio)&#xA;&#x9;FOR MeasureCodeRename IN (' + @cols1 + ')&#xA;)p'&#xA;&#xA;--select @query1&#xA;&#xA;SET @cols2 = REPLACE(@cols, ']', '_ConfidenceIntervalLower]')&#xA;&#xA;set @query2 = 'SELECT p.ProviderId, p.DataYear, ' + @cols2 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, MeasureCode + ''_ConfidenceIntervalLower'' AS MeasureCodeRename, ConfidenceIntervalLower&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[HealthcareAssociatedInfectionScore] mis&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[Measure] m on m.MeasureId = mis.MeasureId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(ConfidenceIntervalLower)&#xA;&#x9;FOR MeasureCodeRename IN (' + @cols2 + ')&#xA;)p'&#xA;&#xA;SET @cols3 = REPLACE(@cols, ']', '_ConfidenceIntervalHigher]')&#xA;&#xA;set @query3 = 'SELECT p.ProviderId, p.DataYear, ' + @cols3 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, MeasureCode + ''_ConfidenceIntervalHigher'' AS MeasureCodeRename, ConfidenceIntervalHigher&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[HealthcareAssociatedInfectionScore] mis&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[Measure] m on m.MeasureId = mis.MeasureId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(ConfidenceIntervalHigher)&#xA;&#x9;FOR MeasureCodeRename IN (' + @cols3 + ')&#xA;)p'&#xA;&#xA;SET @cols4 = REPLACE(@cols, ']', '_Numerator]')&#xA;&#xA;set @query4 = 'SELECT p.ProviderId, p.DataYear, ' + @cols4 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, MeasureCode + ''_Numerator'' AS MeasureCodeRename, Numerator&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[HealthcareAssociatedInfectionScore] mis&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[Measure] m on m.MeasureId = mis.MeasureId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(Numerator)&#xA;&#x9;FOR MeasureCodeRename IN (' + @cols4 + ')&#xA;)p'&#xA;&#xA;SET @cols5 = REPLACE(@cols, ']', '_NumberOfDaysOrPatients]')&#xA;&#xA;set @query5 = 'SELECT p.ProviderId, p.DataYear, ' + @cols5 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, MeasureCode + ''_NumberOfDaysOrPatients'' AS MeasureCodeRename, NumberOfDaysOrPatients&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[HealthcareAssociatedInfectionScore] mis&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[Measure] m on m.MeasureId = mis.MeasureId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(NumberOfDaysOrPatients)&#xA;&#x9;FOR MeasureCodeRename IN (' + @cols5 + ')&#xA;)p'&#xA;&#xA;SET @cols6 = REPLACE(@cols, ']', '_NationalComparisonCategory]')&#xA;&#xA;set @query6 = 'SELECT p.ProviderId, p.DataYear, ' + @cols6 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, MeasureCode + ''_NationalComparisonCategory'' AS MeasureCodeRename, Category AS NationalComparisonCategory&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[HealthcareAssociatedInfectionScore] mis&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[Measure] m on m.MeasureId = mis.MeasureId&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[NationalComparisonCategory] c on c.NationalComparisonCategoryId = mis.NationalComparisonCategoryId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(NationalComparisonCategory)&#xA;&#x9;FOR MeasureCodeRename IN (' + @cols6 + ')&#xA;)p'&#xA;&#xA;SET @queryFinal = 'CREATE VIEW vHealthcareAssociatedInfectionScorePivot AS&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT a.ProviderId, a.DataYear, ' + @cols1 + ', ' + @cols2 + ', ' + @cols3 + ', ' + @cols4 + ', ' + @cols5 + ', ' + @cols6 + '&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;FROM (' + @query1 + ') a &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN (' + @query2 + ') b ON b.ProviderId = a.ProviderId AND b.DataYear = a.DataYear&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN (' + @query3 + ') c ON c.ProviderId = a.ProviderId AND c.DataYear = a.DataYear&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN (' + @query4 + ') d ON d.ProviderId = a.ProviderId AND d.DataYear = a.DataYear&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN (' + @query5 + ') e ON d.ProviderId = e.ProviderId AND e.DataYear = a.DataYear&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN (' + @query6 + ') f ON d.ProviderId = f.ProviderId AND f.DataYear = a.DataYear'&#xA;&#xA;&#xA;&#xA;--select @queryfinal&#xA;execute(@queryFinal)&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
