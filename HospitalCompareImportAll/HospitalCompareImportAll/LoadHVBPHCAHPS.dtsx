                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @Full AS TABLE
(
	ProviderId varchar(10),
	DataYear int,
	BaseScore varchar(100),
	ConsistencyScore varchar(100)
)

INSERT INTO @Full
SELECT [Provider Number]
	  , 2013
      ,[HCAHPS Base Score]
      ,[HCAHPS Consistency Score]
  FROM [HospitalCompareStage].[dbo].[2013_Hvbp_hcahps_05_28_2013]
UNION
SELECT [Provider Number]
	  , 2014
      ,[HCAHPS Base Score]
      ,[HCAHPS Consistency Score]
  FROM [HospitalCompareStage].[dbo].[2014_Hvbp_hcahps_10_28_2014]
UNION
SELECT [Provider Number]
	  , 2015
      ,[HCAHPS Base Score]
      ,[HCAHPS Consistency Score]
  FROM [HospitalCompareStage].[dbo].[2015_Hvbp_hcahps_10_28_2015]
UNION
SELECT [Provider Number]
	  , 2016
      ,[HCAHPS Base Score]
      ,[HCAHPS Consistency Score]
  FROM [HospitalCompareStage].[dbo].[2016_Hvbp_hcahps_06_08_2016]

SELECT f.ProviderId, f.DataYear, f.BaseScore, f.ConsistencyScore From @Full f
INNER JOIN UNRHealthcareData.dbo.HospitalFactGeneral hfg on hfg.ProviderId = f.ProviderID and hfg.DataYear = f.DataYear
ORDER BY f.[ProviderId], f.[DataYear]</property>
