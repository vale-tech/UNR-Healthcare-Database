                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @FULL TABLE
(
	[ProviderId] varchar(10) NOT NULL,
	[DataYear] int NOT NULL,
	[NumberOfDischarges] varchar(100),
	[ExcessReadmissionRatio] varchar(100),
	[PredictedReadmissionRate] varchar(100),
	[ExpectedReadmissionRate] varchar(100),	 
	[NumberOfReadmissions] varchar(100),
	[MeasureCode] varchar(100),
	[MeasureStartDate] datetime,
	[MeasureEndDate] datetime
)
INSERT INTO @Full (ProviderId, DataYear, MeasureCode, NumberOfDischarges, ExcessReadmissionRatio, PredictedReadmissionRate, ExpectedReadmissionRate, NumberOfReadmissions, MeasureStartDate, MeasureEndDate)
SELECT CASE WHEN LEN([Provider Number]) &lt; 6 THEN '0' + [Provider Number] ELSE [Provider Number] END
	  , 2012 AS DataYear
      ,CASE 
		WHEN [Measure Name] = 'Acute Myocardial Infarction (AMI) 30-Day Readmissions' THEN 'READM_30_AMI'
		WHEN [Measure Name] = 'Heart Failure (HF) 30-Day Readmissions' THEN 'READM_30_HF'
		WHEN [Measure Name] = 'Pneumonia (PN) 30-Day Readmissions' THEN 'READM_30_PN'
		ELSE [Measure Name] END AS MeasureCode
      ,[Number of Discharges]
      ,[Excess Readmission Ratio]
      ,[Predicted Readmission Rate]
      ,[Expected Readmission Rate]
      ,[Number of Readmissions]
      ,[Start Date] As MeasureStartDate
      ,[End Date] As MeasureEndDate
  FROM [HospitalCompareStage].[dbo].[2012_HQI_READM_REDUCTION]
UNION
SELECT CASE WHEN LEN([Provider Number]) &lt; 6 THEN '0' + [Provider Number] ELSE [Provider Number] END
	  , 2013 AS DataYear
      ,REPLACE(REPLACE([Measure Name], '-HRRP', ''), '-', '_')
      ,[Number of Discharges]
      ,[Excess Readmission Ratio]
      ,[Predicted Readmission Rate]
      ,[Expected Readmission Rate]
      ,[Number of Readmissions]
      ,[Start Date]
      ,[End Date]
  FROM [HospitalCompareStage].[dbo].[2013_HQI_READM_REDUCTION]
UNION
SELECT[Provider Number]
	  , 2014 AS DataYear
      ,REPLACE(REPLACE([Measure Name], '-HRRP', ''), '-', '_')
      ,[Number of Discharges]
      ,[Excess Readmission Ratio]
      ,[Predicted Readmission Rate]
      ,[Expected Readmission Rate]
      ,[Number of Readmissions]
      ,[Start Date]
      ,[End Date]
  FROM [HospitalCompareStage].[dbo].[2014_HQI_READM_REDUCTION]
UNION
SELECT [Provider_Number]
	  , 2015 AS DataYear
      ,REPLACE(REPLACE([Measure_Name], '-HRRP', ''), '-', '_')
      ,[Number_of_Discharges]
      ,[Excess_Readmission_Ratio]
      ,[Predicted_Readmission_Rate]
      ,[Expected_Readmission_Rate]
      ,[Number_of_Readmissions]
      ,[Start_Date]
      ,[End_Date]
  FROM [HospitalCompareStage].[dbo].[2015_HQI_READM_REDUCTION]
UNION
SELECT[Provider Number]
	  , 2016 AS DataYear
      ,REPLACE(REPLACE([Measure Name], '-HRRP', ''), '-', '_')
      ,[Number of Discharges]
      ,[Excess Readmission Ratio]
      ,[Predicted Readmission Rate]
      ,[Expected Readmission Rate]
      ,[Number of Readmissions]
      ,[Start Date]
      ,[End Date]
  FROM [HospitalCompareStage].[dbo].[2016_HQI_READM_REDUCTION]

SELECT f.ProviderId, f.DataYear, f.NumberOfDischarges, f.ExcessReadmissionRatio, f.PredictedReadmissionRate, f.ExpectedReadmissionRate, f.NumberOfReadmissions, f.MeasureCode, f.MeasureStartDate, f.MeasureEndDate FROM @FULL f 
LEFT JOIN [UNRHealthcareData].dbo.HospitalFactGeneral hf ON hf.ProviderId = f.ProviderId and hf.DataYear = f.DataYear
WHERE hf.ProviderId is not null 
order by f.ProviderId, f.DataYear, f.MeasureCode</property>
