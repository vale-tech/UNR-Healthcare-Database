        <SQLTask:SqlTaskData
          SQLTask:Connection="{519027B6-004D-431F-A052-1086E046AF88}"
          SQLTask:SqlStatementSource="USE [UNRHealthCareData]&#xA;&#xA;DECLARE&#x9;&#xA;@cols AS NVARCHAR(MAX)&#xA;, @cols1 AS NVARCHAR(MAX)&#xA;, @cols2 AS NVARCHAR(MAX)&#xA;, @cols3 AS NVARCHAR(MAX)&#xA;, @cols4 AS NVARCHAR(MAX)&#xA;, @cols5 AS NVARCHAR(MAX)&#xA;, @query1 AS NVARCHAR(MAX)&#xA;, @query2 AS NVARCHAR(MAX)&#xA;, @query3 AS NVARCHAR(MAX)&#xA;, @query4 AS NVARCHAR(MAX)&#xA;, @query5 AS NVARCHAR(MAX)&#xA;, @queryFinal AS NVARCHAR(MAX)&#xA;&#xA;IF OBJECT_ID('vReadmissionReductionScorePivot', 'V') IS NOT NULL&#xA;DROP VIEW vReadmissionReductionScorePivot;&#xA;&#xA;select @cols = STUFF((SELECT ',' + QUOTENAME([MeasureCode]) &#xA;                    FROM [UNRHealthcareData].[dbo].[ReadmissionReductionScore] mis&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[Measure] m on m.MeasureId = mis.MeasureId&#xA;                    group by [MeasureCode]&#xA;            FOR XML PATH(''), TYPE&#xA;            ).value('.', 'NVARCHAR(MAX)') &#xA;        ,1,1,'')&#xA;&#xA;SET @cols1 = REPLACE(@cols, ']', '_NumberOfDischarges]')&#xA;&#xA;set @query1 = 'SELECT p.ProviderId, p.DataYear, ' + @cols1 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, MeasureCode + ''_NumberOfDischarges'' AS MeasureCodeRename, NumberOfDischarges&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[ReadmissionReductionScore] mis&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[Measure] m on m.MeasureId = mis.MeasureId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(NumberOfDischarges)&#xA;&#x9;FOR MeasureCodeRename IN (' + @cols1 + ')&#xA;)p'&#xA;&#xA;--select @query1&#xA;&#xA;SET @cols2 = REPLACE(@cols, ']', '_ExcessReadmissionRatio]')&#xA;&#xA;set @query2 = 'SELECT p.ProviderId, p.DataYear, ' + @cols2 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, MeasureCode + ''_ExcessReadmissionRatio'' AS MeasureCodeRename, ExcessReadmissionRatio&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[ReadmissionReductionScore] mis&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[Measure] m on m.MeasureId = mis.MeasureId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(ExcessReadmissionRatio)&#xA;&#x9;FOR MeasureCodeRename IN (' + @cols2 + ')&#xA;)p'&#xA;&#xA;SET @cols3 = REPLACE(@cols, ']', '_PredictedReadmissionRate]')&#xA;&#xA;set @query3 = 'SELECT p.ProviderId, p.DataYear, ' + @cols3 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, MeasureCode + ''_PredictedReadmissionRate'' AS MeasureCodeRename, PredictedReadmissionRate&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[ReadmissionReductionScore] mis&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[Measure] m on m.MeasureId = mis.MeasureId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(PredictedReadmissionRate)&#xA;&#x9;FOR MeasureCodeRename IN (' + @cols3 + ')&#xA;)p'&#xA;&#xA;SET @cols4 = REPLACE(@cols, ']', '_ExpectedReadmissionRate]')&#xA;&#xA;set @query4 = 'SELECT p.ProviderId, p.DataYear, ' + @cols4 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, MeasureCode + ''_ExpectedReadmissionRate'' AS MeasureCodeRename, ExpectedReadmissionRate&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[ReadmissionReductionScore] mis&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[Measure] m on m.MeasureId = mis.MeasureId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(ExpectedReadmissionRate)&#xA;&#x9;FOR MeasureCodeRename IN (' + @cols4 + ')&#xA;)p'&#xA;&#xA;SET @cols5 = REPLACE(@cols, ']', '_NumberOfReadmissions]')&#xA;&#xA;set @query5 = 'SELECT p.ProviderId, p.DataYear, ' + @cols5 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, MeasureCode + ''_NumberOfReadmissions'' AS MeasureCodeRename, NumberOfReadmissions&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[ReadmissionReductionScore] mis&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[Measure] m on m.MeasureId = mis.MeasureId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(NumberOfReadmissions)&#xA;&#x9;FOR MeasureCodeRename IN (' + @cols5 + ')&#xA;)p'&#xA;&#xA;SET @queryFinal = 'CREATE VIEW vReadmissionReductionScorePivot AS&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT a.ProviderId, a.DataYear, ' + @cols1 + ', ' + @cols2 + ', ' + @cols3 + ', ' + @cols4 + ', ' + @cols5 + '&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;FROM (' + @query1 + ') a &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN (' + @query2 + ') b ON b.ProviderId = a.ProviderId AND b.DataYear = a.DataYear&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN (' + @query3 + ') c ON c.ProviderId = a.ProviderId AND c.DataYear = a.DataYear&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN (' + @query4 + ') d ON d.ProviderId = a.ProviderId AND d.DataYear = a.DataYear&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN (' + @query5 + ') e ON d.ProviderId = e.ProviderId AND e.DataYear = a.DataYear'&#xA;&#xA;&#xA;&#xA;--select @queryfinal&#xA;execute(@queryFinal)&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
