                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @Full Table
(
ProviderID varchar(10),
DataYear int,
MeasureCode varchar(100),
Numerator varchar(100),
Denominator varchar(100),
MeasureStartDate datetime,
MeasureEndDate datetime
)

INSERT INTO @Full
SELECT [PROVIDER_ID]
	  ,2014 AS DataYear
      ,[MEASURE_ID]
      ,[NUMERATOR]
      ,[DENOMINATOR]
      ,CAST([RPTG_PRD_START_DT] AS datetime) AS MeasureStartDate
      ,CAST([RPTG_PRD_END_DT]  AS datetime) AS MeasureEndDate
FROM [HospitalCompareStage].[dbo].[2014_PCH_CANCERSPECIFICMEASURES_HOSPITAL]
UNION
SELECT [PROVIDER_ID]
	  ,2015 AS DataYear
      ,[MEASURE_ID]
      ,[NUMERATOR]
      ,[DENOMINATOR]
      ,[RPTG_PRD_START_DT]
      ,[RPTG_PRD_END_DT]
FROM [HospitalCompareStage].[dbo].[2015_PCH_CANCERSPECIFICMEASURES_HOSPITAL]
UNION
SELECT [PROVIDER_ID]
	  ,2016 AS DataYear
      ,[MEASURE_ID]
      ,[NUMERATOR]
      ,[DENOMINATOR]
      ,[RPTG_PRD_START_DT]
      ,[RPTG_PRD_END_DT]
FROM [HospitalCompareStage].[dbo].[2016_PCH_CANCERSPECIFICMEASURES_HOSPITAL]

 select f.ProviderId, f.DataYear, REPLACE([MeasureCode], '-', '_') AS [MeasureCode], Numerator, Denominator, MeasureStartDate, MeasureEndDate from @Full f
 INNER JOIN UNRHealthcareData.dbo.HospitalFactGeneral hfg on hfg.ProviderId = f.ProviderID and hfg.DataYear = f.DataYear
  ORDER BY [ProviderId], [DataYear]</property>
