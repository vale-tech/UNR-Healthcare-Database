        <SQLTask:SqlTaskData
          SQLTask:Connection="{519027B6-004D-431F-A052-1086E046AF88}"
          SQLTask:SqlStatementSource="USE [UNRHealthCareData]&#xA;&#xA;DECLARE&#x9;&#xA;@cols AS NVARCHAR(MAX)&#xA;, @cols1 AS NVARCHAR(MAX)&#xA;, @cols2 AS NVARCHAR(MAX)&#xA;, @cols3 AS NVARCHAR(MAX)&#xA;, @cols4 AS NVARCHAR(MAX)&#xA;, @cols5 AS NVARCHAR(MAX)&#xA;, @query1 AS NVARCHAR(MAX)&#xA;, @query2 AS NVARCHAR(MAX)&#xA;, @query3 AS NVARCHAR(MAX)&#xA;, @query4 AS NVARCHAR(MAX)&#xA;, @query5 AS NVARCHAR(MAX)&#xA;, @queryFinal AS NVARCHAR(MAX)&#xA;&#xA;IF OBJECT_ID('vTPSScorePivot', 'V') IS NOT NULL&#xA;DROP VIEW vTPSScorePivot;&#xA;&#xA;select @cols = STUFF((SELECT ',' + QUOTENAME(mis.[TPSMeasureId]) &#xA;                    FROM [UNRHealthcareData].[dbo].[TPSScore] mis&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[TPSMeasure] m on m.TPSMeasureId = mis.TPSMeasureId&#xA;                    group by mis.[TPSMeasureId]&#xA;            FOR XML PATH(''), TYPE&#xA;            ).value('.', 'NVARCHAR(MAX)') &#xA;        ,1,1,'')&#xA;&#xA;&#xA;SET @cols1 = REPLACE(@cols, '[', '[TPSId_')&#xA;SET @cols1 = REPLACE(@cols1, ']', '_MeasureScore]')&#xA;&#xA;--select @cols1&#xA;&#xA;&#xA;set @query1 = 'SELECT p.ProviderId, p.DataYear, ' + @cols1 + '&#xA;FROM&#xA;(&#xA;&#x9;SELECT ProviderId, DataYear, ''TPSId_'' + CAST(mis.TPSMeasureId AS nvarchar) + ''_MeasureScore'' AS TPSMeasureIdRename, MeasureScore&#xA;&#x9;FROM [UNRHealthcareData].[dbo].[TPSScore] mis&#x9;&#xA;&#x9;INNER JOIN [UNRHealthcareData].[dbo].[TPSMeasure] m on m.TPSMeasureId = mis.TPSMeasureId&#xA;) x&#xA;PIVOT&#xA;(&#xA;&#x9;MAX(MeasureScore)&#xA;&#x9;FOR TPSMeasureIdRename IN (' + @cols1 + ')&#xA;)p'&#xA;&#xA;--select @query1&#xA;&#xA;&#xA;SET @queryFinal = 'CREATE VIEW vTPSScorePivot AS&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT a.ProviderId, a.DataYear, ' + @cols1 + '&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;FROM (' + @query1 + ') a'&#xA;&#xA;&#xA;&#xA;--select @queryfinal&#xA;execute(@queryFinal)&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
