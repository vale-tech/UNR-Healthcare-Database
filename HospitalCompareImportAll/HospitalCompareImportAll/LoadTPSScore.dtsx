                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @Full AS TABLE
(
	ProviderId varchar(10)
	, DataYear int
	, MeasureName varchar(255)
	, MeasureScore varchar(100)
)

--TPS
INSERT INTO @Full
  SELECT [Provider Number], 2013, MeasureCode, Score
  FROM 
	(SELECT [Provider Number], [Unweighted Normalized Clinical Process of Care Domain Score]
      ,[Weighted Clinical Process of Care Domain Score]
      ,[Unweighted Patient Experience of Care Domain Score]
      ,[Weighted Patient Experience of Care Domain Score]
      ,[Total Performance Score]
      FROM [HospitalCompareStage].[dbo].[2013_Hvbp_tps_05_28_2013]) p
 UNPIVOT
  (Score FOR MeasureCode IN ([Unweighted Normalized Clinical Process of Care Domain Score]
      ,[Weighted Clinical Process of Care Domain Score]
      ,[Unweighted Patient Experience of Care Domain Score]
      ,[Weighted Patient Experience of Care Domain Score]
      ,[Total Performance Score])) AS unpvt

INSERT INTO @Full
  SELECT [Provider Number], 2014, MeasureCode, Score
  FROM 
	(SELECT [Provider Number], [Unweighted Normalized Clinical Process of Care Domain Score]
      ,[Weighted Clinical Process of Care Domain Score]
      ,[Unweighted Patient Experience of Care Domain Score]
      ,[Weighted Patient Experience of Care Domain Score]
      ,[Unweighted Normalized Outcome Domain Score]
      ,[Weighted Outcome Domain Score]
      ,[Unweighted Normalized Efficiency Domain Score]
      ,[Weighted Efficiency Domain Score]
      ,[Total Performance Score]
      FROM [HospitalCompareStage].[dbo].[2014_HVBP_TPS_10_28_2014]) p
 UNPIVOT
  (Score FOR MeasureCode IN ([Unweighted Normalized Clinical Process of Care Domain Score]
      ,[Weighted Clinical Process of Care Domain Score]
      ,[Unweighted Patient Experience of Care Domain Score]
      ,[Weighted Patient Experience of Care Domain Score]
      ,[Unweighted Normalized Outcome Domain Score]
      ,[Weighted Outcome Domain Score]
      ,[Unweighted Normalized Efficiency Domain Score]
      ,[Weighted Efficiency Domain Score]
      ,[Total Performance Score])) AS unpvt

INSERT INTO @Full
  SELECT [Provider Number], 2015, MeasureCode, Score
  FROM 
	(SELECT [Provider Number], [Unweighted Normalized Clinical Process of Care Domain Score]
      ,[Weighted Clinical Process of Care Domain Score]
      ,[Unweighted Patient Experience of Care Domain Score]
      ,[Weighted Patient Experience of Care Domain Score]
      ,[Unweighted Normalized Outcome Domain Score]
      ,[Weighted Outcome Domain Score]
      ,[Unweighted Normalized Efficiency Domain Score]
      ,[Weighted Efficiency Domain Score]
      ,[Total Performance Score]
      FROM [HospitalCompareStage].[dbo].[2015_Hvbp_tps_10_28_2015]) p
 UNPIVOT
  (Score FOR MeasureCode IN ([Unweighted Normalized Clinical Process of Care Domain Score]
      ,[Weighted Clinical Process of Care Domain Score]
      ,[Unweighted Patient Experience of Care Domain Score]
      ,[Weighted Patient Experience of Care Domain Score]
      ,[Unweighted Normalized Outcome Domain Score]
      ,[Weighted Outcome Domain Score]
      ,[Unweighted Normalized Efficiency Domain Score]
      ,[Weighted Efficiency Domain Score]
      ,[Total Performance Score])) AS unpvt


INSERT INTO @Full
  SELECT [Provider Number], 2016, MeasureCode, Score
  FROM 
	(SELECT [Provider Number], [Unweighted Normalized Clinical Process of Care Domain Score]
      ,[Weighted Clinical Process of Care Domain Score]
      ,[Unweighted Patient Experience of Care Domain Score]
      ,[Weighted Patient Experience of Care Domain Score]
      ,[Unweighted Normalized Outcome Domain Score]
      ,[Weighted Outcome Domain Score]
      ,[Unweighted Normalized Efficiency Domain Score]
      ,[Weighted Efficiency Domain Score]
      ,[Total Performance Score]
      FROM [HospitalCompareStage].[dbo].[2016_Hvbp_tps_06_08_2016]) p
 UNPIVOT
  (Score FOR MeasureCode IN ([Unweighted Normalized Clinical Process of Care Domain Score]
      ,[Weighted Clinical Process of Care Domain Score]
      ,[Unweighted Patient Experience of Care Domain Score]
      ,[Weighted Patient Experience of Care Domain Score]
      ,[Unweighted Normalized Outcome Domain Score]
      ,[Weighted Outcome Domain Score]
      ,[Unweighted Normalized Efficiency Domain Score]
      ,[Weighted Efficiency Domain Score]
      ,[Total Performance Score])) AS unpvt
	
SELECT f.ProviderId, f.DataYear, f.MeasureName, f.MeasureScore From @Full f
INNER JOIN UNRHealthcareData.dbo.HospitalFactGeneral hfg on hfg.ProviderId = f.ProviderID and hfg.DataYear = f.DataYear
ORDER BY f.[ProviderId], f.[DataYear], f.MeasureName</property>
